<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.baizhi.mapper.GroupMapper">

    <resultMap id="BaseResultMap" type="com.baizhi.entity.Group">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="name" column="name" jdbcType="VARCHAR"/>
            <result property="createdate" column="createDate" jdbcType="DATE"/>
            <result property="clazzid" column="clazzId" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,name,createDate,
        clazzId
    </sql>
</mapper>
